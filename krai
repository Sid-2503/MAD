import android.widget.TextView; 
import androidx.annotation.NonNull; 
import androidx.annotation.Nullable; 
import java.util.ArrayList; 
public class NumbersViewAdapter extends ArrayAdapter<NumbersView> { 
public NumbersViewAdapter (@NonNull Context context, ArrayList<NumbersView> 
arrayList) { super(context, 0, arrayList); }
@NonNull
@Override
public View getView(int position, @Nullable View convertView, @NonNull ViewGroup 
parent) { View currentItemView = convertView; if 
(currentItemView == null) { 
currentItemView = 
LayoutInflater.from(getContext()).inflate(R.layout.custom_list_view, parent, false); }
 NumbersView currentNumberPosition = getItem(position); 
